---
import { 
  AstroIcon,
  CSS3,
  HTML5,
  JavaScript,
  Preact
} from '@icons/index'
---

<ul aria-labelledby="tecnologias" class="flex items-center flex-wrap content-start gap-2">
  <li title="HTML5">
    <HTML5 class="w-9 h-9" aria-label="HTML5" />
  </li>
  <li title="CSS3">
    <CSS3 class="w-9 h-9" aria-label="CSS3" />
  </li>
  <li title="JavaScript">
    <JavaScript class="w-9 h-9" aria-label="JavaScript" />
  </li>
  <li title="Astro">
    <AstroIcon class="w-9 h-9" aria-label="Astro" />
  </li>
  <li title="Preact">
    <Preact class="w-9 h-9"/>
  </li>
</ul>